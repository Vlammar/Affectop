<?xml version="1.0" encoding="UTF-8"?>
	<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	  <servlet>
	    <servlet-name>Affectop</servlet-name>
	    <servlet-class>com.m1affectop.servlets.Affectop</servlet-class>
	  </servlet>
	  <servlet-mapping>
	    <servlet-name>Affectop</servlet-name>
	    <url-pattern>/bonjour</url-pattern>
	  </servlet-mapping>
	  
	  <servlet>
	    <servlet-name>Acceuil</servlet-name>
	    <servlet-class>com.m1affectop.servlets.Acceuil</servlet-class>
	  </servlet>
	  <servlet-mapping>
	    <servlet-name>Acceuil</servlet-name>
	    <url-pattern>/</url-pattern>
	  </servlet-mapping>
	 
	  <jsp-config>
	    <jsp-property-group>
	      <url-pattern>*.jsp</url-pattern>
	      <include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
	    </jsp-property-group>
	  </jsp-config>
	</web-app>